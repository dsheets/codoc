        then echo '$(DUMPAST) camlp4o $(unit-opamDoc-bin-opam-doc.pp-byte) $(BUILDIR)/bin-opam-doc/opamDoc.ml > $(BUILDIR)/bin-opam-doc/opamDoc.cml-byte'; \
        else echo 'opamDoc.cml-byte          [32m<=[m [01mpp-byte[m unit-opamDoc-bin-opam-doc'; fi
	@$(DUMPAST) camlp4o $(unit-opamDoc-bin-opam-doc.pp-byte) $(BUILDIR)/bin-opam-doc/opamDoc.ml > $(BUILDIR)/bin-opam-doc/opamDoc.cml-byte

